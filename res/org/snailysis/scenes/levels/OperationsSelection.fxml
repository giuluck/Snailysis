<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="700.0" prefWidth="890.0" style="-fx-background-color: #339933;" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ComboBox fx:id="initTrajComboBox" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="40.0" prefWidth="450.0" promptText="Select initial trajectory">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </ComboBox>
            <TilePane fx:id="operationsPane" alignment="CENTER" hgap="60.0" prefHeight="550.0" prefWidth="961.0" style="-fx-background-color: #006600;" vgap="60.0">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </TilePane>
            <HBox alignment="BOTTOM_RIGHT" prefHeight="60.0" prefWidth="901.0">
               <children>
                  <Button fx:id="confirmButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" style="-fx-background-color: #006600;" text="Confirm" textFill="WHITE">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="resetButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" style="-fx-background-color: #006600;" text="Reset" textFill="WHITE" />
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" right="20.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
